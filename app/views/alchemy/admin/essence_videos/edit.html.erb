<%= alchemy_form_for [:admin, @essence_video] do |f| %>
  <%= f.input :width %>
  <%= f.input :height %>
  <%= f.input :autoplay %>
  <%= f.input :controls %>
  <%= f.input :loop %>
  <%= f.input :muted %>
  <%= f.input :preload, collection: %w(auto none metadata),
    include_blank: false, input_html: {class: 'alchemy_selectbox'} %>
  <%= f.submit Alchemy.t(:save) %>
<% end %>
